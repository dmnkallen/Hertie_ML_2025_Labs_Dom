    "linear_model = pm.Model()\n",
    "with linear_model:\n",
    "    # Define Priors\n",
    "    intercept = pm.Normal(\"Intercept\", mu=0, sigma=10)\n",
    "    slope = pm.Normal(\"slope\", mu=0, sigma=10)\n",
    "    sigma = HalfCouchy(\"sigma\", beta=1)\n",
    "\n",
    "    # Define Likelihood\n",
    "    likelihood = Normal(\"y\", mu=intercept + slope * x, sigma=sigma, observed=y)\n",
